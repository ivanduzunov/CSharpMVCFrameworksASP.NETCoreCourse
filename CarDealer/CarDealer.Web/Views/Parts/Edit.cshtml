@model PartsEditModel
@{
    ViewData["Title"] = $"Edit part";
}
<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="Price"></label>
                <input asp-for="Price" class="form-control" id="name">
                <span asp-validation-for="Price" class="form-text text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Quantity"></label>
                <input asp-for="Quantity"  class="form-control" id="price">
                <span asp-validation-for="Quantity" class="form-text text-danger"></span>
            </div>
           
            <input type="submit" class="btn btn-success" value="Edit" />
        </form>
    </div>
</div>
